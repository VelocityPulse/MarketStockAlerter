{
  "name": "MarketStockAlerter",
  "description": "Schedule alarm price of your stocks",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}